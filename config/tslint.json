{
	"rules": {
		"max-line-length": [true, 120],
		//"no-console": [true, "log"],
		"no-var-keyword": true,
		"no-empty-interface": true,
		"no-namespace": true,
		"no-internal-module": true,
		"no-var-requires": true,
		"no-empty": true,
		"no-invalid-this": true,
		"align":true,
		"member-access": true,
		"member-ordering": true,
		"import-spacing": true
	}
}